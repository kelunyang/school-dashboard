{
  "name": "school-dashboard",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:gas": "vite build --config vite.config.gas.js",
    "preview": "vite preview",
    "prepare:gas": "node scripts/buildForGAS.cjs",
    "deploy:gas": "npm run prepare:gas && cd gas-deploy && clasp push",
    "setup:gas": "cd gas-deploy && clasp create --type webapp --title '學校儀表板'",
    "watch:gas": "cd gas-deploy && clasp push --watch",
    "logs:gas": "cd gas-deploy && clasp logs",
    "open:gas": "cd gas-deploy && clasp open --webapp"
  },
